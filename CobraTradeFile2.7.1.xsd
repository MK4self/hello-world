	<!-- ================================================================= -->
	<!--                     Data Type definitions                         -->
	<!-- ================================================================= -->
	<xs:simpleType name="date">
		<xs:annotation>
			<xs:documentation>Represents a date in the format
                YYYYMMDD</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
			<xs:minLength value="8"/>
			<xs:pattern value="((19|20|21)\d\d)(0[1-9]|1[012])(0[1-9]|1[0-9]|2[0-9]|3[01])"/>
			<!-- exactly 8 characters -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="time">
		<xs:annotation>
			<xs:documentation>Represents a time in the format
                HHMMSS</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="6"/>
			<xs:minLength value="6"/>
			<xs:pattern value="(0[0-9]|1[0-9]|2[0-3])([0-5][0-9])([0-5][0-9])"/>
			<!-- exactly 8 characters -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="operationType">
		<xs:annotation>
			<xs:documentation>Operation type - 'N' = New Transactions, 'C' =
                Cancellation</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="16"/>
			<xs:minLength value="1"/>
			<xs:pattern value="[CN]"/>
			<!-- according to the spreadsheet 'spec' -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="brokerCode">
		<xs:annotation>
			<xs:documentation>Representation of a broker code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="16"/>
			<xs:minLength value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="depositTaker">
		<xs:annotation>
			<xs:documentation>Representation of a deposit taker</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="16"/>
			<xs:minLength value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="instrumentType">
		<xs:annotation>
			<xs:documentation>Fixed term load/deposit or call deposit</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="LDP"/>
			<xs:enumeration value="CALL"/>
			<xs:enumeration value="FUND"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="noticePeriod">
		<xs:annotation>
			<xs:documentation>Number of days notice period</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="3"/>
			<xs:fractionDigits value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="balanceAmount">
		<xs:annotation>
			<xs:documentation>Balance call deposit amount following transaction</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="17"/>
			<xs:fractionDigits value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="exchangeCode">
		<xs:annotation>
			<xs:documentation>Representation of a broker code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="4"/>
			<xs:minLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tradeRate">
		<xs:annotation>
			<xs:documentation>fx rate type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="20"/>
			<xs:fractionDigits value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="quantity">
		<xs:annotation>
			<xs:documentation>Quantity type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="20"/>
			<xs:fractionDigits value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="price">
		<xs:annotation>
			<xs:documentation>Price type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="20"/>
			<xs:fractionDigits value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="contractSize">
		<xs:annotation>
			<xs:documentation>Contract size type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="17"/>
			<xs:fractionDigits value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="fxRateType">
		<xs:annotation>
			<xs:documentation>Spot or Forward: ASET = Spot, AFWD =
                Forward</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ASET"/>
			<xs:enumeration value="AFWD"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="openCloseIndicator">
		<xs:annotation>
			<xs:documentation>Open close flag type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1"/>
			<xs:minLength value="1"/>
			<xs:pattern value="[OC]"/>
			<!-- single character only -->
			<!-- allow only a C or an O -->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ftdTransactionType">
		<xs:annotation>
			<xs:documentation>L = Loan, B = Deposit</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="L"/>
			<xs:enumeration value="B"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="secTransactionType">
		<xs:annotation>
			<xs:documentation>ABU = Buy, ASE = Sell</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ABU"/>
			<xs:enumeration value="ASE"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tradeAmount">
		<xs:annotation>
			<xs:documentation>Default type for all trade
                amounts</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="17"/>
			<xs:fractionDigits value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="currency">
		<xs:annotation>
			<xs:documentation>ISO Currency code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="3"/>
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="dayCountFraction">
		<xs:annotation>
			<xs:documentation>TODO: RBC need to confirm</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="16"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="rolloverFlag">
		<xs:annotation>
			<xs:documentation>F = Full Rollover, P = Partial Rollover, N = No
                associated trade for rollover</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1"/>
			<xs:minLength value="1"/>
			<xs:pattern value="[FPN]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tradeRef">
		<xs:annotation>
			<xs:documentation>A trade reference</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="13"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="cancellationIndicator">
		<xs:annotation>
			<xs:documentation>Y = cancellation indicator, N = new block
                record</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1"/>
			<xs:minLength value="1"/>
			<xs:pattern value="[YN]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="rbcSecCode">
		<xs:annotation>
			<xs:documentation>If RBC security Code not provided then
                SEDOL/Instrument Type/Market Code/Security Description/Security
                Currency/Issuer must be present</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="sedol">
		<xs:annotation>
			<xs:documentation>Stock Exchange Daily Official List
