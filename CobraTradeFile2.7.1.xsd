		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="4"/>
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="isin">
		<xs:annotation>
			<xs:documentation>International Securities Identification
                Number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="11"/>
			<xs:maxLength value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="country">
		<xs:annotation>
			<xs:documentation>3-letter ISO Country codes</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="2"/>
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="optionType">
		<xs:annotation>
			<xs:documentation>CAL=call, PUT=put</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="3"/>
			<xs:enumeration value="CAL"/>
			<xs:enumeration value="PUT"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="exCumDividend">
		<xs:annotation>
			<xs:documentation>Y=Ex dividend, C=cum dividend</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="1"/>
			<xs:pattern value="[XC]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="yesNo">
		<xs:annotation>
			<xs:documentation>Y=yes, N=no</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="1"/>
			<xs:pattern value="[YN]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="exCumInterest">
		<xs:annotation>
			<xs:documentation>Y=Ex interest, N=cum interest</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="1"/>
			<xs:pattern value="[YN]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="shareRightsEntitlement">
		<xs:annotation>
			<xs:documentation>C=Cum rights, X=ex rights</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="1"/>
			<xs:pattern value="[CX]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="freeAgainstPayment">
