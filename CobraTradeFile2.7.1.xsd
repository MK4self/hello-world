	<xs:element name="tradeFile" type="rbcTypes:tradefileType" nillable="false"/>
	<xs:complexType name="tradefileType">
		<xs:annotation>
			<xs:documentation>Trade File: contains all trades. Must contain at
                least one</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:choice>
				<xs:element name="rateOption" type="rbcTypes:rateOption"/>
				<xs:element name="ccs" type="rbcTypes:ccs"/>	
				<xs:element name="irswap" type="rbcTypes:irswap"/>
				<xs:element name="cdsOption" type="rbcTypes:cdsOption"/>
				<xs:element name="sec" type="rbcTypes:sec"/>
				<xs:element name="ftd" type="rbcTypes:ftd"/>
				<xs:element name="fx" type="rbcTypes:fx"/>
				<xs:element name="irs" type="rbcTypes:irs"/>
				<xs:element name="cds" type="rbcTypes:cds"/>
				<xs:element name="repo" type="rbcTypes:repo"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="fileName" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="40"/>
					<xs:whiteSpace value="preserve"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dateStamp" type="rbcTypes:date" use="required"/>
		<xs:attribute name="timeStamp" type="rbcTypes:time" use="required"/>
		<xs:attribute name="sequenceNumber" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:nonNegativeInteger">
					<xs:maxInclusive value="99999"/>
					<xs:minInclusive value="00000"/>
					<xs:totalDigits value="5"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="tradeCount" type="xs:int" use="required"/>
	</xs:complexType>
	<!-- ================================================================= -->
	<!--                      Product Type Definitions                     -->
	<!-- ================================================================= -->
	<xs:complexType name="fx">
		<xs:annotation>
			<xs:documentation>Class definition for all Foreign Exchange Product
                Types</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="fxAllocation" type="rbcTypes:fxAllocation" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="fxAllocationCxl" type="rbcTypes:fxAllocation" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ftd">
		<xs:annotation>
			<xs:documentation>Class definition for all Fixed Term Deposit (Money
                Market) Product Types</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ftdAllocation" type="rbcTypes:ftdAllocation" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="ftdAllocationCxl" type="rbcTypes:ftdAllocation" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="sec">
		<xs:annotation>
			<xs:documentation>Class definition for all Security Product
                Types</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="secBlock" type="rbcTypes:secBlock" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="secBlockCxl" type="rbcTypes:secBlock" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="secFundAllocation" type="rbcTypes:secFundAllocation" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="secFundAllocationCxl" type="rbcTypes:secFundAllocation" minOccurs="0" maxOccurs="unbounded"/>
			<xs:choice>
				<xs:element name="secAllocation" type="rbcTypes:secAllocation" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="secChild" type="rbcTypes:secChild" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>			
			<xs:choice>
				<xs:element name="secAllocationCxl" type="rbcTypes:secAllocation" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="secChildCxl" type="rbcTypes:secChild" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>			
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="irs">
		<xs:annotation>
			<xs:documentation>Class definition for all Interest Rate Swap Product types</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="irsAllocation" type="rbcTypes:irsAllocation" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="cds">
		<xs:annotation>
			<xs:documentation>Class definition for all Credit Default Swap Product types</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cdsAllocation" type="rbcTypes:cdsAllocation" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="repo">
		<xs:annotation>
			<xs:documentation>Class definition for all Repo Product types</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="repoAllocation" type="rbcTypes:repoAllocation" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ================================================================= -->
	<!--                      New instrument Product Type Definitions                  -->
	<!-- ================================================================= -->
	<xs:complexType name="cdsOption">
		<xs:annotation>
			<xs:documentation>Class definition for all Credit Default Swap option Product types</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cdsOptionAllocation" type="rbcTypes:cdsOptionAllocation" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="irswap">
		<xs:annotation>
			<xs:documentation>Class definition for all Interest Rate Swap Product types</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="irswapsAllocation" type="rbcTypes:irswapsAllocation" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
		<xs:complexType name="rateOption">
		<xs:annotation>
			<xs:documentation>Class definition for all Rates option Product types</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="rateOptionAllocation" type="rbcTypes:rateOptionAllocation" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ccs">
		<xs:annotation>
			<xs:documentation>Class definition for all cross currency Swap Product types</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ccsAllocation" type="rbcTypes:ccsAllocation" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<!-- ================================================================= -->
	<!--                      Allocation Type Definitions                  -->
	<!-- ================================================================= -->
	<xs:complexType name="allocation">
		<xs:annotation>
			<xs:documentation>Generic allocation, inherited by all concrete
                allocation types (FX, FTD etc)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="source">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="16"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="brokerCode" type="rbcTypes:brokerCode"/>
			<xs:element name="tradeDate" type="rbcTypes:date"/>
			<xs:element name="tradeTime" type="rbcTypes:time"/>
			<xs:element name="portfolioCode">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="6"/>
						<xs:maxLength value="16"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="fxAllocation">
		<xs:annotation>
			<xs:documentation>FX allocation detail</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="rbcTypes:allocation">
				<xs:sequence>
					<xs:element name="tradeRef">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="1"/>
								<xs:maxLength value="13"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="opType" type="rbcTypes:operationType"/>
					<xs:element name="settDate" type="rbcTypes:date"/>
					<xs:element name="purchCurr" type="rbcTypes:currency"/>
					<xs:element name="purchAmt" type="rbcTypes:tradeAmount"/>
					<xs:element name="sellCurr" type="rbcTypes:currency"/>
					<xs:element name="sellAmt" type="rbcTypes:tradeAmount"/>
					<xs:element name="rate" type="rbcTypes:tradeRate"/>
					<xs:element name="flagSpotForward" type="rbcTypes:fxRateType"/>
					<xs:element name="flagOpenClose" type="rbcTypes:openCloseIndicator" minOccurs="0"/>
					<xs:element name="flagOrigTradeRef" type="rbcTypes:tradeRef" minOccurs="0"/>
					<xs:element name="flagShareClassCode" type="rbcTypes:shareClassCode" minOccurs="0" />
					<xs:element name="flagDeliverable" type="rbcTypes:yesNo" minOccurs="0" />
					<xs:element name="fixingDate" type="rbcTypes:date" minOccurs="0" />
					<xs:element name="settCurr" type="rbcTypes:currency" minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ftdAllocation">
		<xs:annotation>
			<xs:documentation>FTD (MM) allocation detail</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="rbcTypes:allocation">
				<xs:sequence>
					<xs:element name="tradeRef">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="1"/>
								<xs:maxLength value="13"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="depositTaker" type="rbcTypes:depositTaker"/>
					<xs:element name="instType" type="rbcTypes:instrumentType"/>
					<xs:element name="noticePeriod" type="rbcTypes:noticePeriod" minOccurs="0"/>
					<xs:element name="balanceAmount" type="rbcTypes:balanceAmount" minOccurs="0"/>
					<xs:element name="opType" type="rbcTypes:operationType"/>
					<xs:element name="tranType" type="rbcTypes:ftdTransactionType"/>
					<xs:element name="startDate" type="rbcTypes:date"/>
					<xs:element name="maturityDate" type="rbcTypes:date" minOccurs="0"/>
					<xs:element name="tradeCurr" type="rbcTypes:currency"/>
					<xs:element name="rate" type="rbcTypes:tradeRate"/>
					<xs:element name="dayCount" type="rbcTypes:dayCountFraction" minOccurs="0"/>
					<xs:element name="prinAmount" type="rbcTypes:tradeAmount"/>
					<xs:element name="intAmount" type="rbcTypes:tradeAmount"/>
					<xs:element name="settAmount" type="rbcTypes:tradeAmount" minOccurs="0"/>
					<xs:element name="flagRollover" type="rbcTypes:rolloverFlag" minOccurs="0"/>
					<xs:element name="origTradeRate" type="rbcTypes:tradeRate" minOccurs="0"/>
					<xs:element name="origTradeRef" type="rbcTypes:tradeRef" minOccurs="0"/>
					<xs:element name="origPrinAmount" type="rbcTypes:tradeAmount" minOccurs="0"/>
					<xs:element name="origMatDate" type="rbcTypes:date" minOccurs="0"/>
					<xs:element name="origIntAmount" type="rbcTypes:tradeAmount" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="secBlock">
		<xs:annotation>
			<xs:documentation>Base type for Securitised Trade allocations and
                blocks</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="source">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="16"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="blockRef">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="13"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="brokerCode" type="rbcTypes:brokerCode"/>
			<xs:element name="clearingBrokerCode" type="rbcTypes:brokerCode" minOccurs="0"/>
			<xs:element name="cancellationInd" type="rbcTypes:cancellationIndicator"/>
			<xs:element name="tranType" type="rbcTypes:secTransactionType"/>
			<xs:element name="tradeDate" type="rbcTypes:date"/>
			<xs:element name="tradeTime" type="rbcTypes:time"/>
			<xs:element name="settDate" type="rbcTypes:date"/>
			<xs:element name="rbcSecCode" type="rbcTypes:rbcSecCode" minOccurs="0"/>
			<xs:element name="sedolCode" type="rbcTypes:sedol" minOccurs="0"/>
			<xs:element name="isinCode" type="rbcTypes:isin" minOccurs="0"/>
			<xs:element name="secDesc" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="instType">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="marketCode">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ctryCode" type="rbcTypes:country"/>
			<xs:element name="secCurr" type="rbcTypes:currency"/>
			<xs:element name="matDate" type="rbcTypes:date" minOccurs="0"/>
			<xs:element name="couponRate" type="rbcTypes:tradeRate" minOccurs="0"/>
			<xs:element name="issuer" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="nextPayDate" type="rbcTypes:date" minOccurs="0"/>
			<xs:element name="numDaysAccrued" type="xs:int" minOccurs="0"/>
			<xs:element name="optionType" type="rbcTypes:optionType" minOccurs="0"/>
			<xs:element name="contractSize" type="rbcTypes:contractSize" minOccurs="0"/>
			<xs:element name="expDate" type="rbcTypes:date" minOccurs="0"/>
			<xs:element name="qty" type="rbcTypes:quantity"/>
			<xs:element name="tradePrice" type="rbcTypes:price"/>
			<xs:element name="prcFactor" type="rbcTypes:price"/>
			<xs:element name="tradeCurr" type="rbcTypes:currency"/>
			<xs:element name="grossAmt" type="rbcTypes:tradeAmount"/>
			<xs:element name="intAmt" type="rbcTypes:tradeAmount" minOccurs="0"/>
			<xs:element name="locFees" type="rbcTypes:tradeAmount" minOccurs="0"/>
			<xs:element name="locTax" type="rbcTypes:tradeAmount" minOccurs="0"/>
			<xs:element name="othFees" type="rbcTypes:tradeAmount" minOccurs="0"/>
			<xs:element name="brokComm" type="rbcTypes:tradeAmount"/>
			<xs:element name="netAmt" type="rbcTypes:tradeAmount"/>
			<xs:element name="settCurr" type="rbcTypes:currency"/>
			<xs:element name="netSettAmt" type="rbcTypes:tradeAmount"/>
			<xs:element name="exchRate" type="rbcTypes:tradeRate" minOccurs="0"/>
			<xs:element name="flagExCumDiv" type="rbcTypes:exCumDividend" minOccurs="0"/>
			<xs:element name="flagExCumInt" type="rbcTypes:exCumInterest" minOccurs="0"/>
			<xs:element name="flagRights" type="rbcTypes:shareRightsEntitlement" minOccurs="0"/>
			<xs:element name="flagOpenClose" type="rbcTypes:openCloseIndicator" minOccurs="0"/>
			<xs:element name="flagShortLong" type="rbcTypes:longShortIndicator" minOccurs="0"/>
			<xs:element name="bargCond" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="flagFreeAgainstPay" type="rbcTypes:freeAgainstPayment" minOccurs="0"/>
			<xs:element name="stratCode" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="flagAgencyPrin" type="rbcTypes:agencyPrincipleIndicator" minOccurs="0"/>
			<xs:element name="flagProgTrade" type="rbcTypes:programTradeIndicator"/>
			<xs:element name="flagIncCap" type="rbcTypes:incomeCapitalIndicator" minOccurs="0"/>
			<xs:element name="flagSoftHard" type="rbcTypes:softHardIndicator" minOccurs="0"/>
			<xs:element name="flagCleanDirty" type="rbcTypes:cleanDirtyIndicator" minOccurs="0"/>
			<xs:element name="flagPlacement" type="rbcTypes:paymentIndicator" minOccurs="0"/>
			<xs:element name="exchangeCode" type="rbcTypes:exchangeCode" minOccurs="0"/>
			<!-- Now adding the CFD fields needed for v2.0.6 -->
			<xs:element name="rateType" type="rbcTypes:rateType" minOccurs="0"/>
			<xs:element name="spread" type="rbcTypes:spread" minOccurs="0"/>
			<xs:element name="rateMatDate" type="rbcTypes:rateMatDate" minOccurs="0"/>
			<xs:element name="delayDays" type="rbcTypes:delayDays" minOccurs="0"/>
			<xs:element name="payDateBasis" type="rbcTypes:payDateBasis" minOccurs="0"/>
			<xs:element name="coEfficient" type="rbcTypes:coEfficient" minOccurs="0"/>
			<xs:element name="resetDate" type="rbcTypes:resetDate" minOccurs="0"/>
			<xs:element name="childCounter" type="rbcTypes:childCounter" minOccurs="0"/>
			<!-- ended adding the V2.0.6 fields -->
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="secChild">
		<xs:annotation>
			<xs:documentation>Child trades that would be matched with the sceBlock 
				through blockRef element</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="source">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="16"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="blockRef">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="13"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="brokerCode" type="rbcTypes:brokerCode"/>
			<xs:element name="cancellationInd" type="rbcTypes:cancellationIndicator"/>
			<xs:element name="tranType" type="rbcTypes:secTransactionType"/>
			<xs:element name="tradeDate" type="rbcTypes:date"/>
			<xs:element name="tradeTime" type="rbcTypes:time"/>
			<xs:element name="settDate" type="rbcTypes:date"/>
			<xs:element name="rbcSecCode" type="rbcTypes:rbcSecCode" minOccurs="0"/>
			<xs:element name="sedolCode" type="rbcTypes:sedol" minOccurs="0"/>
			<xs:element name="isinCode" type="rbcTypes:isin" minOccurs="0"/>
			<xs:element name="secDesc" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>			
			<xs:element name="instType" type="rbcTypes:secInsType"/>
			<xs:element name="marketCode">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ctryCode" type="rbcTypes:country"/>
			<xs:element name="secCurr" type="rbcTypes:currency"/>
			<xs:element name="matDate" type="rbcTypes:date" minOccurs="0"/>
			<xs:element name="couponRate" type="rbcTypes:tradeRate" minOccurs="0"/>
			<xs:element name="issuer" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="nextPayDate" type="rbcTypes:date" minOccurs="0"/>
			<xs:element name="numDaysAccrued" type="xs:int" minOccurs="0"/>
			<xs:element name="qty" type="rbcTypes:quantity"/>
			<xs:element name="tradePrice" type="rbcTypes:price"/>
			<xs:element name="prcFactor" type="rbcTypes:price"/>
			<xs:element name="tradeCurr" type="rbcTypes:currency"/>
			<xs:element name="grossAmt" type="rbcTypes:tradeAmount"/>
			<xs:element name="intAmt" type="rbcTypes:tradeAmount" minOccurs="0"/>
			<xs:element name="locFees" type="rbcTypes:tradeAmount" minOccurs="0"/>
			<xs:element name="locTax" type="rbcTypes:tradeAmount" minOccurs="0"/>
			<xs:element name="othFees" type="rbcTypes:tradeAmount" minOccurs="0"/>
			<xs:element name="brokComm" type="rbcTypes:tradeAmount"/>
			<xs:element name="netAmt" type="rbcTypes:tradeAmount"/>
			<xs:element name="settCurr" type="rbcTypes:currency"/>
			<xs:element name="netSettAmt" type="rbcTypes:tradeAmount"/>
			<xs:element name="exchRate" type="rbcTypes:tradeRate" minOccurs="0"/>
			<xs:element name="flagExCumDiv" type="rbcTypes:exCumDividend" minOccurs="0"/>
			<xs:element name="flagExCumInt" type="rbcTypes:exCumInterest" minOccurs="0"/>
			<xs:element name="flagRights" type="rbcTypes:shareRightsEntitlement" minOccurs="0"/>
			<xs:element name="bargCond" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
